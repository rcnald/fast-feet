{
  "extends": ["@rocketseat/eslint-config/node"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "semi": false
      }
    ],
    "camelcase": "off",
    "no-useless-constructor":"off",
    "@typescript-eslint/no-explicit-any": "off",
    "no-new": "off",
    "@typescript-eslint/no-unused-vars": "warn"
  }
}